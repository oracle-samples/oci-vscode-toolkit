  "activationEvents": [
    "onView:apm",
    "onCommand:apm.refreshTree",
    "onCommand:apm.openIssueInGithub",
    "onCommand:apm.launch"
  ],
  "main": "./dist/extension",
  "contributes": {
    "_comment": "Please place images in resources folder & refer them in the below command's icon property as 'resources/oci-icon.png'",
