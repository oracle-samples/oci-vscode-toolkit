  },
  "categories": [
    "Other"
  ],
  "publisher": "Oracle",
  "homepage": "https://docs.oracle.com/en-us/iaas/application-performance-monitoring/home.htm",
  "repository": {
    "type": "git",
    "url": "https://github.com/oracle-samples/oci-vscode-toolkit",
    "directory": "src/apm-vscode-plugin"
  },
  "license": "UPL",
  "icon": "resources/oracle-icon.png",
  "bugs": {
    "url": "https://github.com/oracle-samples/oci-vscode-toolkit/issues"
  },
  "galleryBanner": {
    "color": "#FFFFFF",
    "theme": "light"
  },
  "keywords": [
    "Oracle",
    "OCI",
    "Application Performance Monitoring",
    "apm",
    "availability monitoring",
    "synthetics",
    "json",
    "typescript"
