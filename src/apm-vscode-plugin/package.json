    "views": {
      "oci": [
        {
          "id": "apm",
          "name": "%apm.view.title%"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "apm",
        "when": "!cloudEditorEnv && !OCIConfigExists",
        "contents": "Looks like you don't have an OCI account.\n[Create an OCI account](command:oci-core.newOCIAccount)\n[Sign in to an existing account](command:oci-core.createNewProfile)"
      },
      {
        "view": "apm",
        "when": "(cloudEditorEnv || OCIConfigExists) && UnAuthorizedAccess",
        "contents": "\nAuthorization failed or requested resource not found.\nPlease check if resource exists and has proper policy setup under tenancy"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "apm.refreshTree",
          "when": "view == apm && enableApmSyntheticsViewTitleMenus",
          "group": "navigation"
        },
        {
          "command": "apm.openIssueInGithub",
          "when": "view == apm && (!cloudEditorEnv) && enableApmSyntheticsViewTitleMenus",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "apm.listVantagePoints",
          "when": "view == apm && viewItem == MonitorsNode",
          "group": "1@1"
        },
        {
          "command": "apm.createOCIAPMSynMonitor",
          "when": "view == apm && viewItem == MonitorsNode",
          "group": "1@2"
        },
        {
          "command": "apm.getOCIAPMSynMonitorDetails",
          "when": "view == apm && viewItem == OCIApmSynMonitorSummaryNode",
          "group": "1@1"
        },
        {
          "command": "apm.viewMonitorInBrowser",
          "when": "view == apm && viewItem == OCIApmSynMonitorSummaryNode",
          "group": "1@2"
        },
        {
          "command": "apm.editOCIAPMSynMonitor",
          "when": "view == apm && viewItem == OCIApmSynMonitorSummaryNode",
          "group": "1@3"
        },
        {
          "command": "apm.runNowOCIAPMSynMonitor",
          "when": "view == apm && viewItem == OCIApmSynMonitorSummaryNode",
          "group": "1@4"
        },
        {
          "command": "apm.copyMonitorOCID",
          "when": "view == apm && viewItem == OCIApmSynMonitorSummaryNode",
          "group": "1@5"
        },
        {
          "command": "apm.deleteOCIAPMSynMonitor",
          "when": "view == apm && viewItem == OCIApmSynMonitorSummaryNode",
          "group": "1@6"
        },
        {
          "submenu": "apm.executionResults",
          "group": "navigation && 1@7"
        },
        {
          "command": "apm.createOCIAPMSynScript",
          "when": "view == apm && viewItem == ScriptsNode",
          "group": "1@1"
        },
        {
          "command": "apm.getOCIAPMSynScriptDetails",
          "when": "view == apm && viewItem == OCIApmSynScriptSummaryNode",
          "group": "1@1"
        },
        {
          "command": "apm.viewScriptInBrowser",
          "when": "view == apm && viewItem == OCIApmSynScriptSummaryNode",
          "group": "1@2"
        },
        {
          "command": "apm.downloadOCIAPMSynScript",
          "when": "view == apm && viewItem == OCIApmSynScriptSummaryNode",
          "group": "1@3"
        },
        {
          "command": "apm.editOCIAPMSynScript",
          "when": "view == apm && viewItem == OCIApmSynScriptSummaryNode",
          "group": "1@4"
        },
        {
          "command": "apm.copyScriptOCID",
          "when": "view == apm && viewItem == OCIApmSynScriptSummaryNode",
          "group": "1@5"
        },
        {
          "command": "apm.deleteOCIAPMSynScript",
          "when": "view == apm && viewItem == OCIApmSynScriptSummaryNode",
          "group": "1@6"
        },
        {
          "command": "apm.createOCIAPMSynOnPremiseVantagePoint",
          "when": "view == apm && viewItem == OnPremiseVantagePointsNode",
          "group": "1@1"
        },
        {
          "command": "apm.createWorkerOCIAPMSynOnPremiseVantagePoint",
          "when": "view == apm && viewItem == OnPremiseVantagePointsNode",
          "group": "1@2"
        },
        {
          "submenu": "apm.downloadWorkerOCIAPMSynOPVP",
          "group": "navigation && 1@3"
        },
        {
          "command": "apm.getAPMSynOnPremiseVantagePointResults",
          "when": "view == apm && viewItem == OCIApmOpvpNode",
          "group": "1@1"
        },
        {
          "command": "apm.deleteOCIAPMSynOnPremiseVantagePoint",
          "when": "view == apm && viewItem == OCIApmOpvpNode",
          "group": "1@2"
        },
        {
          "command": "apm.createOCIAPMSynWorker",
          "when": "view == apm && viewItem == WorkersNode",
          "group": "1@1"
        },
        {
          "command": "apm.getAPMSynWorkerResults",
          "when": "view == apm && viewItem == OCIApmSynWorkerSummaryNode",
          "group": "1@1"
        },
        {
          "command": "apm.disableOCIAPMSynWorker",
          "when": "view == apm && viewItem == OCIApmSynWorkerSummaryNode",
          "group": "1@2"
        },
        {
          "command": "apm.deleteOCIAPMSynWorker",
          "when": "view == apm && viewItem == OCIApmSynWorkerSummaryNode",
          "group": "1@3"
        }
      ],
      "apm.downloadWorkerOCIAPMSynOPVP": [
        {
          "command": "apm.downloadRestWorkerOCIAPMSynOPVP",
          "when": "view == apm && viewItem == OnPremiseVantagePointsNode",
          "group": "navigation && 1@1"
        },
        {
          "command": "apm.downloadSideWorkerOCIAPMSynOPVP",
          "when": "view == apm && viewItem == OnPremiseVantagePointsNode",
          "group": "navigation && 1@2"
        }
      ],
      "apm.executionResults": [
        {
          "command": "apm.getAPMSynMonitorResults",
          "when": "view == apm && viewItem == OCIApmSynMonitorSummaryNode",
          "group": "navigation && 1@1"
        },
        {
          "command": "apm.viewErrorMessage",
          "when": "view == apm && viewItem == OCIApmSynMonitorSummaryNode",
          "group": "navigation && 1@2"
        },
        {
          "command": "apm.viewHar",
          "when": "view == apm && viewItem == OCIApmSynMonitorSummaryNode",
          "group": "navigation && 1@3"
        },
        {
          "command": "apm.viewScreenshots",
          "when": "view == apm && viewItem == OCIApmSynMonitorSummaryNode",
          "group": "navigation && 1@5"
        },
        {
          "command": "apm.getLogs",
          "when": "view == apm && viewItem == OCIApmSynMonitorSummaryNode",
          "group": "navigation && 1@6"
        }
      ],
